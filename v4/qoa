if [ -z "$1" ]; then echo "Hướng dẫn sử dụng lệnh qoa (Mở nhanh ứng dụng)" && echo 'Gõ lệnh "qoa v2" để mở v2rayNG' && echo 'Gõ lệnh "qoa neko" để mở nekobox' && echo 'Gõ lệnh "qoa tun2" để mở tun2tap' && echo 'Gõ lệnh "qoa hyper" để mở hypersocksdroid'; else case "$1" in v2) clear && echo "Đang khởi động v2rayng..." && am start -n com.v2ray.ang/com.v2ray.ang.ui.MainActivity && echo "Đã khởi động v2rayng" && v111man ;; neko) clear && echo "Ðang khởi động nekobox..." && am start -n moe.nb4a/io.nekohasekai.sagernet.ui.MainActivity && echo "Đã khởi động nekobox" && v111man ;; tun2) clear && echo "Đang khởi động tun2tap..." && am start -n com.newtoolsworks.tun2tap/com.newtoolsworks.vpn2share.MainActivity && echo "Đã khởi động tun2tap" && v111man ;; hyper) clear && echo "Đang khởi động hypersocksdroid..." && am start -n net.typeblog.socks/net.typeblog.socks.MainActivity && echo "Đã khởi động hypersocksdroid" && v111man ;; *) clear && echo 'Vui lòng nhập lại, cú pháp là "wl <mã app>".' && v111man ;; esac fi